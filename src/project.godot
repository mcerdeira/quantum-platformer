config_version=5

[application]

config/name="gruta-demo"
config/version="1.0"
run/main_scene="res://scenes/main.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
boot_splash/bg_color=Color(0.141176, 0.141176, 0.141176, 1)
boot_splash/fullsize=false
boot_splash/use_filter=false

[autoload]

Global="*res://scripts/Global.gd"
Music="*res://scripts/Music.gd"
Ambience="*res://scripts/Music.gd"

[display]

window/size/viewport_height=640
window/size/resizable=false
window/stretch/mode="viewport"
window/stretch/aspect="expand"

[editor]

movie_writer/movie_file="C:/Develop/Godot/gameplay.avi"

[file_customization]

folder_colors={
"res://scenes/levels/challenge/": "pink",
"res://scenes/levels/dragon/": "red",
"res://scenes/levels/leaf/": "green",
"res://scenes/levels/mermaid/": "purple",
"res://scenes/levels/tomb/": "gray"
}

